azul-fondo = #001633
azul-titulo = #346287
sub-menu = #00285B
subtitulo = #445566
bg-label = #fcfcfc
blanco = #FFFFFF
body-color = #f9f9f9
border = #F1F1F1
border-gris = #cccccc
*{
	-webkit-moz-box-sizing border-box
	-moz-box-sizing border-box
	-ms-box-sizing border-box
	-o-box-sizing border-box
	box-sizing border-box
}
@font-face {
	font-family: 'icon';
	src:url('../fonts/icomoon.eot?t6fcdt');
	src:url('../fonts/icomoon.eot?t6fcdt#iefix') format('embedded-opentype'),
		url('../fonts/icomoon.ttf?t6fcdt') format('truetype'),
		url('../fonts/icomoon.woff?t6fcdt') format('woff'),
		url('../fonts/icomoon.svg?t6fcdt#icomoon') format('svg');
	font-weight: normal;
	font-style: normal;
}

@font-face {
    font-family: 'ubuntu';
    src: url('../fonts/ubuntu-r-webfont.eot');
    src: url('../fonts/ubuntu-r-webfont.eot?#iefix') format('embedded-opentype'),
         url('../fonts/ubuntu-r-webfont.woff2') format('woff2'),
         url('../fonts/ubuntu-r-webfont.woff') format('woff'),
         url('../fonts/ubuntu-r-webfont.ttf') format('truetype'),
         url('../fonts/ubuntu-r-webfont.svg#ubunturegular') format('svg');
    font-weight: normal;
    font-style: normal;

}
@font-face {
    font-family: 'roboto';
    src: url('../fonts/roboto.eot');
    src: url('../fonts/roboto.eot?#iefix') format('embedded-opentype'),
         url('../fonts/roboto.woff2') format('woff2'),
         url('../fonts/roboto.woff') format('woff'),
         url('../fonts/roboto.ttf') format('truetype'),
         url('../fonts/roboto.svg#roboto') format('svg');
 }

body
	background azul-fondo
	font-family 'roboto', 'Helvetica', 'sans-serif'
	height auto
	font-size 16px
	margin 0
.content_global
	// background url(../img/pattpdi2.png)
	background #FAFAFA
	margin 0 auto
	max-width 1400px
	min-width 300px
	min-height 700px
	width 100%
	position absolute
h1
	color azul-fondo
header
	background #FAFAFA
	border-bottom 2px solid #F1F1F1
	display block
	height auto
	width 100%
	nav
		font-size 0
		height auto
		margin 0 auto
		.cont_menu
			vertical-align top
			ul
				display table
				// background #F8F8F8
				border 1px solid #e5e5e5
				border-radius 3px
				height 30px
				list-style none
				padding 0
				li
					display table-cell
					text-align center
					a
						color #022E5B
						border-bottom 2px solid blanco
						display block
						font-size 14px
						line-height 20px
						padding 5px
						text-decoration none
						&:hover
							border-bottom 2px solid #022E5B
							color blanco
							background #022E5B
					.ico
						padding-right 3px
						font-size 20px

		.logo
			img
				width 100%
footer
	padding 0
	width 100%
	position relative
	.pie_pag
		height auto
		margin 30px auto
		padding 3px
		width 210px
		img
			display block
			margin 0 auto
		.submenu
			width 100%
			margin-top 3px
			a
				font-size 14px
				display inline-block
				color gray
				text-decoration none
				padding 2px 5px
		small
			font-size 12px
.body_content
	background border
	display block
	margin 0px auto
	min-height 400px
	position relative
	h1
		color #346287
		font-size 1.4em
	.contenedor-menu
		background azul-fondo
		display inline-block
		font-family 'roboto', sans-serif
		line-height 18px
		overflow-y auto
		ul
			list-style none
			padding 0
		.btn-menu
			background #0d2c44
			line-height 18px
			color blanco
			padding 20px
			text-decoration none
			.icono
				float right
		.menu
			font-size 0
			width 100%
			padding 0
			
			ul
				display none
				li
					a
						background sub-menu
						color #e9e9e9
						font-size 12px
						padding 10px 20px
			li
				font-size 16px
				padding 0
				margin 0
				list-style none
				a
					display block
					font-family 'roboto'
					text-decoration none
					line-height 16px
					color blanco
					padding 15px 20px
					background #001633
					&:hover
						background blanco
						color azul-fondo
			.activado > a
				background blanco
				color azul-fondo
			.icono
				font-size 12px
				line-height 16px
			.izquierda
				float left
				margin-right 10px
			.derecha
				float right
				margin-left 10px
	
	.contenedor-section
		background blanco
		border 1px #f4f4f4 solid
		display inline-block
		height auto
		padding 5px
		vertical-align top
		// width 84.5555%
		section
			border 1px border solid
			border-radius 5px 5px 0px 0px
			// display inline-block
			height 200px
			vertical-align top
			// width 49.5555%
			div
				border-bottom 1px solid border
				background #f7f7f7
				span
					// position relative
					color subtitulo
					display inline-block
					font-size 1em
					font-weight 600
					letter-spacing 1px
					line-height 20px
					margin 0
					padding-left 5%
					width 43%
					i
						padding-right 3px
						font-size 20px
				input
					border 1px border solid
					border-radius: 5px;
					font-size 14px
				.buscar
					padding: 5px 5px 5px 28px;
				.i-buscar
					margin-left 7px
					position absolute
					margin-top 8px
					color #dbdbdb
				.sub-form
					color subtitulo
					font-weight 600
					font-size 1em
					letter-spacing 1px
					margin 0
					padding 10px 0px
					display inline-block
			.subtitle
				position relative
				.izq
					padding-left 15px
					font-size 20px
					color subtitulo
			.c-info
				width 100%
				padding 5px
				background bg-label
				.c-input
					// display inline-block
					// width 49.5555%
					padding 0
					background bg-label
					line-height 30px
					span
						padding 0
						width auto
						margin 0
						input
							padding 0px 2px
							border-radius 0px
							font-size 14px
							font-weight normal
							width 85px
						select
							font-weight 100
							font-size 12px
						// .i-fecha
						// 	border-color #d8d8d8
					.d-name
						width 140px
						padding-left 15px
					.u-name
						font-size 14px
						font-weight normal
				.c-text
					background bg-label
					textarea
						width 100%
						padding 5px
						border 1px solid border
						min-height 400px
						margin-bottom 5px
					.b-input
						width 120px
						padding 5px 3px
						font-size 1em
						font-weight bold
						letter-spacing 1px
					.b-izq
						background #334455
						color blanco
						&:hover
							background #44BBFF
					.b-der
						color gray
						&:hover
							background #FC575E
							color blanco
				.msg
					background #DEF0A5
					color #A3C148
					line-height 30px
					font-size 20px
					padding 10px 30px
					border-radius 30px
					width 35%
					margin-top 10px
					font-weight bold
				.msg-error
					color #DD381D
					background #FFD4CC
					line-height 30px
					font-size 20px
					padding 10px 30px
					border-radius 30px
					width 35%
					margin-top 10px
					font-weight bold
				.msg-i
					font-size 25px
				.msg-v
					background #D6D6D6
					color blanco
					line-height 30px
					font-size 20px
					padding 10px 30px
					border-radius 30px
					width 35%
					margin-top 10px
					font-weight bold				
		.item-usuario, .item-titulos
			overflow-y auto
			ul
				padding 0
				font-size 0
				background red
				list-style none
				margin 0
				width 100%
				display block
				li
					background bg-label
					cursor pointer
					font-size 14px
					line-height 16px
					text-transform uppercase
					padding 5px 10px
					&:nth-child(2n+1)
						background blanco
					&:hover
						background subtitulo
						color blanco
		.contenedor-form
			width 100%
			margin-top 16px
			height auto
@media screen and (min-width: 300px)
	.cont_menu
		display none
		ul
			width 100%
	.logo
		min-width 80px
		max-width 100px
		margin 0 auto
	.contenedor-menu
		margin 0
		width 100%
		z-index 1000
		.btn-menu
			display block
		.menu
			display none
	.contenedor-section
		width 100%
		.item-usuario, .item-titulos
			display block
			.subtitle
				.ocultar
					display none
		.contenedor-form
			.c-info
				.c-input
					display block
					width 100%
					select
						width 130px
@media screen and (min-width: 370px)
	.contenedor-section
		.item-usuario, .item-titulos
			.subtitle
				.ocultar
					display inline-block
		.contenedor-form
			.c-info
				.c-input
					select
						width 150px
@media screen and (min-width: 700px)
	.contenedor-section
		.contenedor-form
			.c-info
				.c-input
					display inline-block
					width 49.5555%
		
@media screen and (min-width: 750px)
	.contenedor-section
		.item-titulos, .item-usuario
			display inline-block
			width 49.5555%

@media screen and (min-width: 1000px)
	nav
		display table
		height 75px
		width 75%
		div
			display table-cell
		.cont_menu
			display block
			height 75px
			position relative
			margin-top 0
			right 0
			ul
				bottom 10px
				position absolute
				right 0px
				width 36%
		.logo
			width 120px
			height 75px
	.contenedor-menu
		width 200px
		height 600px
		.btn-menu
			display none
		.menu
			display block
	.contenedor-section
		width 79.5555%
@media screen and (min-width: 1050px)
	.contenedor-section
		width 80%
@media screen and (min-width: 1100px)
	.contenedor-section
		width 81%
@media screen and (min-width: 1150px)
	.contenedor-section
		width 82%
@media screen and (min-width: 1200px)
	.contenedor-section
		width 83%
@media screen and (min-width: 1280px)
	.contenedor-section
		width 84%
@media screen and (min-width: 1321px)
	.contenedor-section
		width 84.5555%